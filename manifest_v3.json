{
  "manifest_version": 3,
  "name": "YouTube Stats",
  "version": "1.0",
  "action": { "default_popup": "popup.html" },
  "permissions": ["tabs"],
  "host_permissions": [
    "https://*.youtube.com/*",
    "https://youtu.be/*",
    "https://www.googleapis.com/*",
    "http://localhost:5003/*"
  ]
}